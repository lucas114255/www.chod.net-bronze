# 青铜器物件120
bo:120
    a crm:E22_Human-Made_Object ;
    crm:P1_is_identified_by
        [ a crm:E33_E41_Linguistic_Appellation ;
            crm:P190_has_symbolic_content "象尊" ; 
            crm:P72_has_language aat:300388113 ;
