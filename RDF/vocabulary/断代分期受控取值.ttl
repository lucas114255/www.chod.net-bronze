@prefix skos: <http://www.w3.org/2004/02/skos/core> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix bvp:<http://www.chod.net/bronze/vocabulary/period/> .

<http://www.chod.net/bronze/vocabulary/period/> a skos:Concept ;
    skos:prefLabel "年代取值"@zh ;
    skos:narrower bvp:01,bvp:02,bvp:03,bvp:04,bvp:05,bvp:06,bvp:07,bvp:08,bvp:09,bvp:10.

bvp:01 a skos:Concept ;
    rdfs:label "夏";
    rdfs:comment "禹~癸".

bvp:02 a skos:Concept;
    rdfs:label "早商或中商时期";
    rdfs:comment "汤~盘庚（迁殷前）".

bvp:03 a skos:Concept;
    rdfs:label "晚商时期";
    rdfs:comment "盘庚、小辛、小乙、武丁、祖庚、祖甲、廪辛、康丁、武乙、文丁、帝乙、帝辛".

bvp:04 a skos:Concept;
    rdfs:label "西周早期";
    rdfs:comment "武、成、康、昭".

bvp:05 a skos:Concept;
    rdfs:label "西周中期";
    rdfs:comment "穆、恭、懿、孝、夷".

bvp:06 a skos:Concept;
    rdfs:label "西周晚期";
    rdfs:comment "厉、宣、幽".

bvp:07 a skos:Concept;
    rdfs:label "春秋".

bvp:08 a skos:Concept;
    rdfs:label "战国".

bvp:09 a skos:Concept;
    rdfs:label "秦代".

bvp:10 a skos:Concept;
    rdfs:label "汉代".
